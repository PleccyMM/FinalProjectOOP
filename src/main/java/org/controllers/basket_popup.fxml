<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.image.ImageView?>
<?import org.vexillum.ToggleSwitch?>
<?import javafx.collections.FXCollections?>

<VBox fx:id="boxDarkening"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      stylesheets="@MainStyle.css"
      style="-fx-background-color: #D9D9D966">
    <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
        <VBox fx:id="boxContainment" styleClass="white-divider" spacing="10" alignment="CENTER" maxWidth="300">
            <HBox id="blue-fill" minHeight="32" alignment="CENTER">
                <padding>
                    <Insets right="8" top="8" left="8" bottom="8"/>
                </padding>
                <Label id="subhead-text" text="Additional Information"/>
            </HBox>
            <VBox alignment="CENTER" spacing="15">
                <padding>
                    <Insets right="8" top="8" left="8" bottom="16"/>
                </padding>
                <HBox spacing="4">
                    <Label fx:id="lblAdditionalDesc" styleClass="mid-text-black"/>
                    <Label fx:id="lblAdditionalVal" styleClass="mid-text-black"/>
                </HBox>
                <HBox spacing="4">
                    <Label fx:id="lblMaterialDesc" styleClass="mid-text-black"/>
                    <Label fx:id="lblMaterialVal" styleClass="mid-text-black"/>
                </HBox>
                <Button fx:id="btnBlue"
                        minWidth="150"
                        minHeight="35"
                        style="-fx-font-size: 14; -fx-background-radius: 15;"
                        text="Return"/>
            </VBox>
        </VBox>
    </VBox>
</VBox>