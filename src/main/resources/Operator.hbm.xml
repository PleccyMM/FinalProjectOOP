<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "org.vexillum.Operator" table = "operators">
        <id name = "operatorID" column = "operatorid" type = "int" />
        <property name = "name" column = "name" type = "string"/>
        <property name = "password" column = "password" type = "string"/>
        <property name = "approved" column = "approved" type = "boolean"/>
        <property name = "administrator" column = "administrator" type = "boolean"/>
    </class>
</hibernate-mapping>